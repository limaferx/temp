package hello_world;

public class HelloWorld {

	public static void main(String[] args) {
		int[] x = {0, 2, -4, 2};
		int count = 0;
		for(int i = 0; i < x.length; i++) {
			if(x[i] >= 0) {
				System.out.println(count + "acrescimo");
				count++;
				
			}
			
		}
		System.out.println(count + "resultado");
	}

}
