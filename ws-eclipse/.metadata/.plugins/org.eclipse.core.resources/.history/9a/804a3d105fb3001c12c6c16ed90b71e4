package hello_world;

public class HelloWorld {

	public static void main(String[] args) {
		int[] x = {0, 1, 0};
		boolean verZero;
		for(int i = 0; i < x.length; i++) {
			if(x[i] == 0) {
				System.out.println(i);
				if(x[i] != 0) {
					verZero = true;
					if(verZero == true) {
						System.out.println(-1);
					}
				}
			}
		}
		
	}

}
