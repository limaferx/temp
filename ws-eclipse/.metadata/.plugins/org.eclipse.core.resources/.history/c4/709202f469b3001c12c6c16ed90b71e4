package hello_world;

public class HelloWorld {

	public static void main(String[] args) {
		int[] x = {2, 3, 5};
		int y = 2, count = 0;
		for(int i = x.length-1; i > 0; i--) {
			if(x[i] == y) {
				System.out.println(i);;
			}else {
				count++;
			}
		}
		if(count == x.length) {
		System.out.println(-1);
		}
	}
}
